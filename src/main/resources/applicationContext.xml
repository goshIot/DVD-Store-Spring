<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd" default-autowire="byType">

    <bean id="defaultMovieService" class="com.mycompany.dvdstore.service.DefaultMovieService"/>

    <bean id="fileMovieRepository" class="com.mycompany.dvdstore.repository.FileMovieRepository">
        <property name="file" value ="movies.csv"/>
    </bean>

    <bean id="movieController" class="com.mycompany.dvdstore.controller.MovieController"/>


</beans>